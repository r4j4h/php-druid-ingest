{
    "name": "r4j4h/php-druid-ingest",
    "description": "PHP Classes to wrap ingestion of data into Druid",
    "keywords": ["druid", "indexing", "ingestion", "overlord"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jasmine Hegman",
            "email": "hegpetz@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "psr/log": "^1.0",
        "guzzle/guzzle": "~3.9",
        "symfony/console": "~2.5",
        "r4j4h/php-druid-query": "1.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "phing/phing": "~2",
        "phpmd/phpmd": "~2",
        "phploc/phploc": "~2"
    },
    "autoload": {
        "psr-4": {
            "PhpDruidIngest\\": "src/",
            "ReferralIngester\\Command\\": "clicommands/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpDruidIngest\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.1.x-dev",
            "dev-master": "1.0.x-dev"
        }
    }
}
